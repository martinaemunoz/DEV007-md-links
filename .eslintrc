{
  "env": {
    "jest": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },

  "extends": [
    "airbnb"
  ],

  "rules": {
    "max-len": ["error", { "code": 80 }],
    "no-console": "off"
  },

  "overrides": [
    {
      "files": ["**/*.spec.js", "**/*.spec.jsx"]
    }
  ]
}
